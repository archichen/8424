<view>
  <view class='title'>
    {{selectorRange[selectIndex]}}
  </view>

  <view class='echarts-container'>
      <ec-canvas id="mychart-dom-bar" canvas-id="mychart-bar" ec="{{ ec }}"></ec-canvas>
  </view>

  <view class='setting' bindtap='openSetting'>
  <picker mode='selector' bindchange='doChange' range='{{selectorRange}}'>
    <view>
      <image class='setting-btn' src='../assets/setting.png'></image>
    </view>
  </picker>
  </view>
